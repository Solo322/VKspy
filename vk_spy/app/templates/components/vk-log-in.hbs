<button {{action "logIn"}}>Войти</button>
<button {{action "currentUser"}}>Текущий пользователь</button>

<ul>
  {{#each authUsers.users as |item|}}
    <li>
      {{item.fullName}}
      <button {{action "selectUser" item}}>Выбрать</button>
      <button {{action "removeUser" item}}>Удалить</button>
    </li>
  {{/each}}
</ul>